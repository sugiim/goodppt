<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no"/>

  <script src="/assets/jquery-2.1.0.min.js" type="text/javascript"></script>
  <script src="/assets/json2.js" type="text/javascript"></script>
  <script src="/assets/underscore-min.js" type="text/javascript"></script>
  <script src="/assets/backbone-min.js" type="text/javascript"></script>


  <style type="text/css">
    @-webkit-keyframes ripple {
        0% {
            opacity: 1;
            -webkit-transform: scale(0);
        }
        100% {
            opacity: 0;
            -webkit-transform: scale(1);
        }
    }

    .ripple {
        width: 500px;
        height: 500px;
        border-radius: 250px;
        background-image: -webkit-gradient(
            radial,
            center center, 0,
            center center, 300,
            from(rgba(252,243,28,0)),
            to(rgba(252,243,28,1))
        );
        -webkit-animation-name: ripple;
        -webkit-animation-iteration-count: 1;
        -webkit-animation-duration: 1s;
        -webkit-animation-timing-function: ease-out;
    }

  </style>

<title>presen List</title>
</head>
<body>
<div id="votelt">
  <div data-role="page">
    <div data-role="header">
      <h3>IT-Suiken LT</h3>
    </div>
    <div role="main" class="ui-content">
      <ul data-role="listview" role="listbox" id="presen-list"> </ul>
    </div>
  </div>


  <div data-role="dialog" id="detail_dialog">
    <div data-role="header">
      <h1 id="ppt_name"></h1>
    </div>
    <div data-role="content">
      <a data-role="button" id="good">＋１</a>
    </div>
  </div>
</div>






  <!-- Templates -->
  <script type="text/template" id="item-template">

        <a href="" data-role="button" data-inline="true" data-rel="dialog" data-transition="pop" class="popDetail" id="presenlist">
        <h3>{{ppt_name}} /  {{user_name}}  </h3><p><span class="ui-li-count" id="count_{{ppt_id}}">{{point}}</span></p>
        </a>
  </script>

  <script type="text/template" id="detail-template">

  </script>


  <script src="/assets/app.js" type="text/javascript"></script>
  <script src="http://code.jquery.com/mobile/1.4.2/jquery.mobile-1.4.2.min.js" type="text/javascript"></script>
  <script src="/assets/jquery.mobile-1.4.2.js" type="text/javascript"></script>

  <link rel="stylesheet" href="http://code.jquery.com/mobile/1.4.2/jquery.mobile-1.4.2.min.css" type="text/css"/> 
  <link rel="stylesheet" href="/assets/jquery.mobile-1.4.2.css" type="text/css"/>


</body>

</html>

